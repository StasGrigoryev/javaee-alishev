<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>LibraryServlet</servlet-name>
    <servlet-class>org.example.javaeetest.servlets.LibraryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LibraryServlet</servlet-name>
    <url-pattern>/library</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SetCookiesServlet</servlet-name>
    <servlet-class>org.example.javaeetest.servlets.SetCookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetCookiesServlet</servlet-name>
    <url-pattern>/setCookies</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetCookiesServlet</servlet-name>
    <servlet-class>org.example.javaeetest.servlets.GetCookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetCookiesServlet</servlet-name>
    <url-pattern>/getCookies</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteCookiesServlet</servlet-name>
    <servlet-class>org.example.javaeetest.servlets.DeleteCookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteCookiesServlet</servlet-name>
    <url-pattern>/deleteCookies</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>org.example.javaeetest.servlets.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/new</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>org.example.javaeetest.servlets.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>greeting</servlet-name>
    <jsp-file>/greeting.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>greeting</servlet-name>
    <url-pattern>/greeting</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>users</servlet-name>
    <jsp-file>/users.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>users</servlet-name>
    <url-pattern>/users</url-pattern>
  </servlet-mapping>
